    "cookie": "^0.4.1",
    "crypto-js": "^4.0.0",
    "got": "^11.6.2",
    "lodash": "^4.17.20",
    "long": "^4.0.0",
    "protobufjs": "^6.10.1",
    "querystring": "^0.2.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/lodash": "^4.14.188",
    "@types/websocket": "^1.0.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
